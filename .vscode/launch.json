{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: app.main",
      "justMyCode": false,
      "request": "launch",
      "module": "app.main",
      "console": "integratedTerminal",
      "python": "venv/bin/python3",
       "type": "debugpy"
    },    
    {
      "name": "analyze_issues",
      "justMyCode": false,
      "request": "launch",
      "program": "app/analyze_issues.py",
      "console": "integratedTerminal",
      "python": "venv/bin/python3",
      "type": "debugpy",
      "redirectOutput": true,
    },
    {
      "name": "Python Debugger: app.create_repo",
      "justMyCode": false,
      "request": "launch",
      "module": "app.create_repo",
      "console": "integratedTerminal",
      "python": "venv/bin/python3",
       "type": "debugpy"
    },    
    {
      "name": "REGRESS Python Debugger: Current File",
      "justMyCode": false,
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "python": "venv/bin/python3",
      "type": "debugpy",
      "redirectOutput": true,
    }
  ]
}